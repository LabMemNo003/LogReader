{
    "manifest_version": 2,
    "name": "Log Reader",
    "version": "1.3.2",
    "description": "Log Reader",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "commands": {
        "trigger": {
            "suggested_key": {
                "default": "Ctrl+Shift+F"
            },
            "description": "trigger"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*",
                "file://*/*"
            ],
            "js": [
                "utility.js",
                "content.js",
                "content_hl.js",
                "content_ce.js",
                "content_sp.js"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "options_page": "options.html"
}