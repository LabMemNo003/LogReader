{
    "manifest_version": 2,
    "name": "Log Reader",
    "version": "1.1.0",
    "description": "Log Reader",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": [
            "worker_api/worker_proxy.js",
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "file://*/*"
            ],
            "js": [
                "worker_api/worker_proxy.js",
                "worker_api/worker_api.js",
                "utility.js",
                "content.js"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "worker_api/worker_proxy.html"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "options_page": "options.html"
}